   "source": [
    "## Escape Sequences"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 80,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "John Smith \n",
      "is a Person\n"
     ]
    }
   ],
   "source": [
    "print(\"John Smith \\nis a Person\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 81,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "John Smith \\is a Person\n"
     ]
    }
   ],
   "source": [
    "print(\"John Smith \\\\is a Person\")"
   ]
